{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.cjs"
    },
    {
      "src": "/(.*\\.[a-zA-Z0-9]+)$",
      "dest": "dist/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.cjs"
    }
  ]
}