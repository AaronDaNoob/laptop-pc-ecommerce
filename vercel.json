{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1",
      "check": true,
      "status": 200
    }
  ],
  "functions": {
    "server/index.ts": {
      "maxDuration": 60
    }
  }
}